<tr>
  <td><%= check_box_tag("ids[]", bib.id, checked, id: "ids_#{bib.id}", class: "select-row") if can?(:update, bib) %></td>
  <td><%= link_to bib.name, bib %></td>
  <td><%= bib.try!(:doi) %></td>
  <td><%= bib.authors.map {|author| author.name }.join(" & ")%></td>
  <td><%= bib.try!(:year) %></td>
  <td><%= bib.try!(:entry_type) %></td>
  <td><%= bib.user.try!(:username) %></td>
  <td><%= bib.group.try!(:name) %></td>
  <td><%= difference_from_now(bib.published_at) %></td>
  <td><%= difference_from_now(bib.updated_at) %></td>
  <td><%= difference_from_now(bib.created_at) %></td>
</tr>

