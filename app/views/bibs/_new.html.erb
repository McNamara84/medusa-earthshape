<%= form_for bib, remote: true, format: :json, html: { class: "create" } do |f| %>
<tr>
  <th></th>
  <th><%= f.text_field :name %></th>
  <th><%= f.text_field :doi %></th>
  <th></th>
  <th><%= f.text_field :year %></th>
  <th><%= f.text_field :entry_type %></th>
  <th>
    <%= f.button class: "btn btn-default" do %>
    <span class="glyphicon glyphicon-save"></span>
    <% end %>
  </th>
  <th></th>
</tr>
<% end %>

